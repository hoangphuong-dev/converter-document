{
    "name": "phuonght/converter-document",
    "description": "Support converting documents to other formats",
    "type": "library",
    "authors": [
        {
            "name": "User Name",
            "email": "example@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Colombo\\Converters\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "symfony/process": "^6.0 || ^7.0",
        "symfony/finder": "^6.0 || ^7.0",
        "symfony/mime": "^6.0 || ^7.0",
        "guzzlehttp/guzzle": "^7.0",
        "nelexa/zip": "^4.0",
        "hocvt/php-apache-tika": "^0.9.2",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^6.0 || ^7.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
